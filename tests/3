write 0 0 00000000
write 0 1 11111111
write 0 2 22222222
write 0 3 33333333
read 0 0 8//should print 00000000
read 0 1 8//should print 11111111
read 0 2 8//should print 22222222
read 0 3 8//should print 33333333
readToFile 0 0 8 p0.txt//should print 00000000 to file p0.txt
readToFile 0 1 8 p1.txt//should print 11111111 to file p1.txt
readToFile 0 2 8 p2.txt//should print 22222222 to file p2.txt
readToFile 0 3 8 p3.txt//should print 33333333 to file p3.txt
exit//should kill all the processes and only than exit
